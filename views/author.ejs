    </div>

    <!-- Starting right frame nav bar  -->
    <div class="right_frame">
      <div class="right-nav-bar">
        <div class="day-night-mode">
          <a href="#">
            <img src="icons/Contrast.svg" alt="">
          </a>
        </div>
        <div class="nav-bar">
          <ul>
            <li>
              <a href="#"><img src="icons/Home.svg" alt=""></a>
            </li>
            <li>
              <a href="#"><img src="icons/Service.svg" alt=""></a>
            </li>
            <li>
              <a href="#"><img src="icons/Cv.svg" alt=""></a>
            </li>
            <li>
              <a href="#"><img src="icons/Portfolio.svg" alt=""></a>
            </li>
            <li>
              <a href="#"><img src="icons/Blog.svg" alt=""></a>
            </li>
            <li>
              <a href="#"><img src="icons/Contact.svg" alt=""></a>
            </li>
          </ul>
