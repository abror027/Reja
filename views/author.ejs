<body>
  <!-- SHU YERDAN YOZASIZ -->

  <div class="main">

    <!-- Starting left a side frame -->
    <div class="left_frame">
      <div class="left-frame-main">
        <div class="profile-ceo">
          <div class="left-header">
            <div class="header-pic-name">
              <img src="images/Profile.png" alt="">
              <p>
                <%=user.name%>
              </p>
              <p>
                <%=user.profession%>
              </p>
            </div>
          </div>
          <div class="marcos-icon-link">
            <a href="#">
              <img src="icons/Icons-facebook.svg" alt="">
            </a>
            <a href="#">
              <img src="icons/Icons-instagram.svg" alt="">
            </a>
            <a href="#">
              <img src="icons/Icons-twitter.svg" alt="">
            </a>
            <a href="#">
              <img src="icons/Icons-linkedin.svg" alt="">
            </a>
            <a href="#">
              <img src="icons/Icons-YouTube.svg" alt="">
            </a>
            <a href="#">
              <img src="icons/Icons-dribbble.svg" alt="">
            </a>
          </div>
          <div class="border-radius-all-one"></div>
          <div class="info-user-profile">
            <div class="age">
              <p>Age:</p>
              <p>24</p>
            </div>
            <div class="residence">
              <p>Residence:</p>
              <p>
                <%=user.residence%>
              </p>
            </div>
            <div class="freelanse">
              <p>Freelance:</p>
              <p>Available</p>
            </div>
            <div class="adress">
              <p>Address:</p>
              <p>
                <%=user.adress%>
              </p>
            </div>
            <div class="border-radius-all-one"></div>
          </div>
          <div class="languages">
            <h3>Languages</h3>
            <div class="first">
              <div class="name-lang">
                <p>
                  <%=user.name%>
                </p>
                <p>100%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="second">
              <div class="name-lang">
                <p>Korean</p>
                <p>72%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="thread">
              <div class="name-lang">
                <p>English</p>
                <p>37%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="border-radius-all-one"></div>
          </div>
          <div class="skills">
            <h3>Skills</h3>
            <div class="first">
              <div class="name-lang">
                <p>Html</p>
                <p>90%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="second">
              <div class="name-lang">
                <p>CSS</p>
                <p>65%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="the-3">
              <div class="name-lang">
                <p>Js</p>
                <p>45%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="the-4">
              <div class="name-lang">
                <p>PHP</p>
                <p>0%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="the-5">
              <div class="name-lang">
                <p>NodeJs</p>
                <p>17%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="the-6">
              <div class="name-lang">
                <p>WordPress</p>
                <p>75%</p>
              </div>
              <div class="point-procent">
                <div class="in-point"></div>
              </div>
            </div>
            <div class="border-radius-all-one"></div>
          </div>
          <div class="extra-skills">
            <h3>Extra Skills</h3>
            <div class="box">
              <a href="#">
                <img src="icons/Icons.svg" alt="">
                <p>Bootstrap, Materialize</p>
              </a>
            </div>
            <div class="box">
              <a href="#">
                <img src="icons/Icons.svg" alt="">
                <p>Stylus, Sass, Less</p>
              </a>
            </div>
            <div class="box">
              <a href="#">
                <img src="icons/Icons.svg" alt="">
                <p>Gulp, Webpack, Grunt</p>
              </a>
            </div>
            <div class="box">
              <a href="#">
                <img src="icons/Icons.svg" alt="">
                <p>GIT Knowledge</p>
              </a>
            </div>
            <div class="border-radius-all-one"></div>
          </div>
        </div>
      </div>
    </div>

    <!-- Starting middle main frame -->
    <div class="mid_frame">
      <main>
        <div class="header">
          <div class="header-title">
            <h1>Doctor neurologists
